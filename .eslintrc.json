{
  "extends": ["eslint:recommended", "plugin:react/recommended",
  "plugin:react/jsx-runtime",
  "plugin:tailwindcss/recommended"],
  "env": {
    "node": true,
    "es2021": true,
  },
  "rules": {
    "tailwindcss/classnames-order": "on"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "tailwindcss"
  ],
}